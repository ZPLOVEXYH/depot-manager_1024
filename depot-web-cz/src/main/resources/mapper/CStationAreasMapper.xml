<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.samples.depot.web.cz.mapper.CStationAreasMapper">
    <resultMap id="BaseResultMap" type="cn.samples.depot.web.entity.CStationAreas">
        <id column="id" jdbcType="VARCHAR" property="id"/>        <!--  -->
        <result column="code" jdbcType="VARCHAR" property="code"/>    <!-- 堆区编码 -->
        <result column="name" jdbcType="VARCHAR" property="name"/>    <!-- 堆区名称 -->
        <result column="station_code" jdbcType="VARCHAR" property="stationCode"/>    <!-- 所属场站 -->
        <result column="row_no" jdbcType="VARCHAR" property="rowNo"/>    <!-- 贝 -->
        <result column="column_no" jdbcType="VARCHAR" property="columnNo"/>    <!-- 列 -->
        <result column="layer_no" jdbcType="VARCHAR" property="layerNo"/>    <!-- 行 -->
        <result column="enable" jdbcType="INTEGER" property="enable"/>    <!-- 是否启用 -->
        <result column="remark" jdbcType="VARCHAR" property="remark"/>    <!-- 备注 -->
        <result column="create_time" jdbcType="BIGINT" property="createTime"/>    <!-- 创建时间 -->
    </resultMap>

    <sql id="Base_Column_List">
		id, code, name, station_code, row_no,column_no,layer_no,enable, remark, create_time
	</sql>
</mapper>